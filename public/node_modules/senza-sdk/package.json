{
    "name": "senza-sdk",
    "version": "3.20.6",
    "main": "./dist/bundle.js",
    "description": "API for Senza application",
    "license": "MIT",
    "files": [
        "dist/"
    ],
    "homepage": "https://senza.synamedia.com",
    "scripts": {
        "clean": "rm -rf dist",
        "prepublish": "npm run build",
        "eslint": "eslint --max-warnings 0  src test",
        "build": "npx webpack --config webpack.config.js",
        "test": "jest --coverage"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "7.18.6",
        "@babel/plugin-transform-modules-commonjs": "^7.16.8",
        "@babel/preset-env": "^7.14.1",
        "babel-jest": "^27.5.1",
        "eslint": "^8.40.0",
        "eslint-plugin-jest": "^26.1.1",
        "jest": "^27.5.1",
        "jsdoc-to-markdown": "^7.1.1",
        "webpack-cli": "^5.1.4",
        "webpack": "^5.72.1"
    },
    "jest": {
        "verbose": false,
        "testEnvironment": "jsdom",
        "moduleDirectories": [
            "src",
            "node_modules"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 84,
                "functions": 89,
                "lines": 91,
                "statements": 91
            }
        }
    }
}
